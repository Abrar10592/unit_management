      <TableView fx:id="PARTS_STAT_TABLE" layoutX="-1.0" layoutY="55.0" prefHeight="480.0" prefWidth="926.0">
         <columns>
            <TableColumn fx:id="entry" prefWidth="75.0" text="ENTRY NO" />
            <TableColumn fx:id="parts_tail" prefWidth="75.0" text="TAIL NO" />
            <TableColumn fx:id="modelname" prefWidth="112.800048828125" text="MODEL" />
            <TableColumn fx:id="company" prefWidth="119.20001220703125" text="COMPANY NAME" />
            <TableColumn fx:id="next_r_d" prefWidth="99.2000732421875" text="NEXT REPAIR" />
            <TableColumn fx:id="prchaser" prefWidth="112.79998779296875" text="PURCHASE BY" />
            <TableColumn fx:id="lasr_reason" prefWidth="132.7999267578125" text="last overhaul reason" />
            <TableColumn fx:id="PHL" prefWidth="122.39990234375" text="HOURS LEFT" />
            <TableColumn fx:id="P_STAT" prefWidth="75.0" text="STATUS" />
         </columns>
      </TableView>
      <TextField fx:id="parts_search" layoutX="14.0" layoutY="555.0" prefHeight="26.0" prefWidth="346.0" promptText="search by entry no or aircraft tail no" />
      <Button fx:id="search_parts" layoutX="401.0" layoutY="556.0" mnemonicParsing="false" onAction="#search_partsf" text="search" />
